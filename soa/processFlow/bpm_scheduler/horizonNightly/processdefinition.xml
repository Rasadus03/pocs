<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="horizonNightly">


	<start-state name="startHorizonNightly">
		<transition to="fork1"></transition>
	</start-state>


	<process-state name="udsProcessFlow">
		<sub-process name="udsProcessFlow" binding="late"></sub-process>
		<transition to="join1"></transition>
	</process-state>

	<process-state name="sqlServerFlow">
		<sub-process name="sqlServerFlow" binding="late"></sub-process>
		<transition to="join1"></transition>
	</process-state>

	<join name="join1">
		<transition to="endHorizonNightly" name="toEndHorizonNightly"></transition>
	</join>

	<fork name="fork1">
		<transition to="sqlServerFlow" name="toSqlServerFlow"></transition>
		<transition to="udsProcessFlow" name="toUdsProcessFlow"></transition>
	</fork>


	<end-state name="endHorizonNightly"></end-state>


	<event type="process-start">
		<script>
			System.out.println(&quot;horizonNightly process-start&quot;);
		</script>
	</event>

	<event type="process-end">
		<script>
			System.out.println(&quot;horizonNightly process-end&quot;);
		</script>
	</event>


</process-definition>
