<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd" xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.2.xsd" xmlns:csv="http://www.milyn.org/xsd/smooks/csv-1.2.xsd">

   <csv:reader fields="voucherId, propertyId, projectId, projectName, voucherType, managementEntity, businessUnit, commitmentNumber, vendorInvoiceNumber, vendorInvoiceDate, companyNumber, Description, vendorLocationCode, invoiceAmount, actualsItemNumber, actualsItemDescription, accountCode, actualsItemAmount, paymentId, paymentAmount" recordElementName="csv-record" skipLines="1" />
  <jb:bean beanId="voucherList" class="java.util.ArrayList" createOnElement="csv-set">
        <jb:wiring beanIdRef="voucher" />
  </jb:bean>
  <jb:bean beanId="voucherLineList" class="java.util.ArrayList" createOnElement="csv-set">
        <jb:wiring beanIdRef="voucherLine" />
  </jb:bean>
  <jb:bean beanId="voucher" class="org.jboss.Voucher" createOnElement="csv-record">
    <jb:value data="csv-record/businessUnit" property="businessUnit"/>
    <jb:value data="csv-record/commitmentNumber" property="commitmentNumber"/>
    <jb:value data="csv-record/companyNumber" property="companyNumber"/>
    <jb:value data="csv-record/Description" property="description"/>
    <jb:value data="csv-record/invoiceAmount" decoder="Double" property="invoiceAmount"/>
    <jb:value data="csv-record/managementEntity" property="managementEntity"/>
    <jb:value data="csv-record/projectId" property="projectId"/>
    <jb:value data="csv-record/projectName" property="projectName"/>
    <jb:value data="csv-record/propertyId" property="propertyId"/>
    <jb:value data="csv-record/vendorInvoiceDate" decoder="Date" property="vendorInvoiceDate"/>
    <jb:value data="csv-record/vendorInvoiceNumber" property="vendorInvoiceNumber"/>
    <jb:value data="csv-record/vendorLocationCode" property="vendorLocationCode"/>
    <jb:value data="csv-record/voucherId" property="voucherId"/>
    <jb:value data="csv-record/voucherType" property="voucherType"/>
  </jb:bean>
  <jb:bean beanId="voucherLine" class="org.jboss.VoucherLine" createOnElement="csv-record">
    <jb:value data="csv-record/voucherId" property="voucherId"/>
    <jb:value data="csv-record/propertyId" property="propertyId"/>
    <jb:value data="csv-record/projectId" property="projectId"/>
    <jb:value data="csv-record/accountCode" property="accountCode"/>
    <jb:value data="csv-record/actualsItemAmount" decoder="Double" property="actualsItemAmount"/>
    <jb:value data="csv-record/actualsItemDescription" property="actualsItemDescription"/>
    <jb:value data="csv-record/actualsItemNumber" decoder="Integer" property="actualsItemNumber"/>
    <jb:value data="csv-record/paymentAmount" property="paymentAmount"/>
    <jb:value data="csv-record/paymentId" property="paymentId"/>
  </jb:bean>

</smooks-resource-list>
